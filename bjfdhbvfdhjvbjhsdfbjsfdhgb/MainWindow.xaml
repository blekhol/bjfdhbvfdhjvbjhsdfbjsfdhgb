<Window x:Class="bjfdhbvfdhjvbjhsdfbjsfdhgb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bjfdhbvfdhjvbjhsdfbjsfdhgb"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

	<Canvas x:Name="canvas">
		<Canvas.Background>
			<ImageBrush ImageSource="a.png" Stretch="UniformToFill"/>
		</Canvas.Background>


		<Ellipse x:Name="madar" Height="28" Canvas.Left="111" Stroke="Black" Canvas.Top="155" Width="50" HorizontalAlignment="Left" VerticalAlignment="Top" Fill="#FFFFDE00"/>

		<GroupBox BorderThickness="0" Canvas.Left="825" Canvas.Top="-65" HorizontalAlignment="Left" VerticalAlignment="Top">
			<StackPanel>
				<Rectangle Height="281" Stroke="Black" Width="62" HorizontalAlignment="Center" VerticalAlignment="Top" Fill="#FF2BFF00"/>
				<Rectangle Height="30" Stroke="Black" Width="74" HorizontalAlignment="Center" VerticalAlignment="Top" Fill="#FF2BFF00" />
			</StackPanel>
		</GroupBox>

		<GroupBox BorderThickness="0" Canvas.Left="825" Canvas.Top="375" HorizontalAlignment="Center" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
			<GroupBox.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform Angle="180"/>
					<TranslateTransform/>
				</TransformGroup>
			</GroupBox.RenderTransform>
			<StackPanel>
				<Rectangle Height="281" Canvas.Left="380" Stroke="Black" Canvas.Top="172" Width="62" HorizontalAlignment="Center" VerticalAlignment="Top" Fill="#FF2BFF00"/>
				<Rectangle Height="30" Canvas.Left="374" Stroke="Black" Canvas.Top="169" Width="74" HorizontalAlignment="Center" VerticalAlignment="Top" Fill="#FF2BFF00" />
			</StackPanel>
		</GroupBox>


	</Canvas>
</Window>
